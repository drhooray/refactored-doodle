@Library('refactored-doodle')_
pipeline {
    agent any

    parameters {
	choice(name: 'GIT_TAG', choices: ['20241206', '20250108_1730_388'], description: 'Select the Git tag to build')
    }

    stages {
        stage('Clone Repo') {
            steps {
		cloneRepo(params.GIT_TAG)
            }
        }
    }
}
